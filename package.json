{
  "name": "better-dialog",
  "version": "1.0.0",
  "description": "A advanced template with eslint + prettier",
  "main": "index.js",
  "scripts": {
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "build": "npm run lint:check && npm run format:check",
    "build:fix": "npm run lint:fix && npm run format:fix",
    "dev": "nodemon .",
    "test": "npm run build:fix"
  },
  "keywords": [],
  "author": "hvlxh",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.1.4",
    "better-dialog": "file:",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8"
  }
}
